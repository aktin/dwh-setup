UPDATE WORK_DB_LOOKUP SET C_DOMAIN_ID = '@I2B2HIVEID@', C_DB_DATASOURCE = 'java:/Workplace@I2B2PROJECTNAME@DS';
UPDATE ONT_DB_LOOKUP SET C_DOMAIN_ID = '@I2B2HIVEID@', C_DB_DATASOURCE = 'java:/Ontology@I2B2PROJECTNAME@DS';
UPDATE CRC_DB_LOOKUP SET C_DOMAIN_ID = '@I2B2HIVEID@', C_DB_DATASOURCE = 'java:/QueryTool@I2B2PROJECTNAME@DS';
UPDATE IM_DB_LOOKUP SET C_DOMAIN_ID = '@I2B2HIVEID@', C_DB_DATASOURCE = 'java:/IM@I2B2PROJECTNAME@DS';


UPDATE WORK_DB_LOOKUP SET c_db_fullschema = '@I2B2WORKUSER@' WHERE c_db_fullschema = 'public';
UPDATE ONT_DB_LOOKUP SET c_db_fullschema = '@I2B2METAUSER@' WHERE c_db_fullschema = 'public';
UPDATE CRC_DB_LOOKUP SET c_db_fullschema = '@I2B2DEMOUSER@' WHERE c_db_fullschema = 'public';
UPDATE IM_DB_LOOKUP SET c_db_fullschema = '@I2B2IMUSER@' WHERE c_db_fullschema = 'public';


Insert into WORK_DB_LOOKUP (C_DOMAIN_ID,C_PROJECT_PATH,C_OWNER_ID,C_DB_FULLSCHEMA,C_DB_DATASOURCE,C_DB_SERVERTYPE,C_DB_NICENAME) 
	SELECT '@I2B2HIVEID@','@I2B2PROJECTNAME@/','@','@I2B2WORKUSER@','java:/Workplace@I2B2PROJECTNAME@DS','@DBTYPE@','@I2B2PROJECTNAME@'
	WHERE
		NOT EXISTS (SELECT 1 FROM WORK_DB_LOOKUP WHERE C_DOMAIN_ID='@I2B2HIVEID@');
Insert into ONT_DB_LOOKUP (C_DOMAIN_ID,C_PROJECT_PATH,C_OWNER_ID,C_DB_FULLSCHEMA,C_DB_DATASOURCE,C_DB_SERVERTYPE,C_DB_NICENAME) 
	SELECT '@I2B2HIVEID@','@I2B2PROJECTNAME@/','@','@I2B2METAUSER@','java:/Ontology@I2B2PROJECTNAME@DS','@DBTYPE@','@I2B2PROJECTNAME@'
	WHERE
		NOT EXISTS (SELECT 1 FROM ONT_DB_LOOKUP WHERE C_DOMAIN_ID='@I2B2HIVEID@');
Insert into CRC_DB_LOOKUP (C_DOMAIN_ID,C_PROJECT_PATH,C_OWNER_ID,C_DB_FULLSCHEMA,C_DB_DATASOURCE,C_DB_SERVERTYPE,C_DB_NICENAME) 
	SELECT '@I2B2HIVEID@','@I2B2PROJECTNAME@/','@','@I2B2DEMOUSER@','java:/QueryTool@I2B2PROJECTNAME@DS','@DBTYPE@','@I2B2PROJECTNAME@'
	WHERE
		NOT EXISTS (SELECT 1 FROM CRC_DB_LOOKUP WHERE C_DOMAIN_ID='@I2B2HIVEID@');
Insert into IM_DB_LOOKUP (C_DOMAIN_ID,C_PROJECT_PATH,C_OWNER_ID,C_DB_FULLSCHEMA,C_DB_DATASOURCE,C_DB_SERVERTYPE,C_DB_NICENAME) 
	SELECT '@I2B2HIVEID@','@I2B2PROJECTNAME@/','@','@I2B2IMUSER@','java:/IM@I2B2PROJECTNAME@DS','@DBTYPE@','@I2B2PROJECTNAME@'
